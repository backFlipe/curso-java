package entities;

public class Numbers {
	
	private int quantity;
	private double number;
	
	public Numbers (int quantity, double number) {
		this.quantity = quantity;
		this.number = number;
		
	}

	public double getNumber() {
		return number;
	}

	public void setNumber(double number) {
		this.number = number;
	}
	
	
}
