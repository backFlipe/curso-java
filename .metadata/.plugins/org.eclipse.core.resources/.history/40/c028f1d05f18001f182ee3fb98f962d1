package exMatriz1;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner (System.in);
		
		System.out.println("Insira o n√∫mero de linhas e colunas: ");
		int n = sc.nextInt();
		int m = sc.nextInt();
		
		System.out.println(n + "" + m);
		
		sc.close();
	}

}
